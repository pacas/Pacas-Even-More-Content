<Defs>
	<!--============================================-->
	<ThingDef ParentName="PEMC_BaseTool">
		<defName>Dll_Tool_Chainsaw</defName>
		<label>chainsaw</label>
		<description>Portable saw that cuts with a set of rotating chain with teeth. Hard to use as a weapon, but if enemy is right here, and you don't have a gun...</description>
		<graphicData>
			<texPath>Things/Weapons/Tools/Chainsaw</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<stuffCategories>
			<li>StrongMetallic</li>
            <li>RuggedMetallic</li>
		</stuffCategories>
		<costStuffCount>95</costStuffCount>
		<costList>
			<ComponentIndustrial>10</ComponentIndustrial>
			<Mechanism>15</Mechanism>
			<Plastic>35</Plastic>
		</costList>
		<statBases>
			<WorkToMake>9000</WorkToMake>
			<Bulk>6.5</Bulk>
			<Mass>6</Mass>
			<MeleeCounterParryBonus>0.1</MeleeCounterParryBonus>
            <ToolEffectivenessFactor>1</ToolEffectivenessFactor>
		</statBases>
		<!--<equippedAngleOffset>-35</equippedAngleOffset>-->
		<techLevel>Industrial</techLevel>
		<weaponTags>
			<li>Handaxe</li>
			<li>MidworldMelee</li>
		</weaponTags>
		<tools>
			<li Class="CombatExtended.ToolCE">
				<label>handle</label>
				<capacities>
					<li>Poke</li>
				</capacities>
				<power>4</power>
				<cooldownTime>2.25</cooldownTime>
				<chanceFactor>0.4</chanceFactor>
				<armorPenetrationBlunt>2</armorPenetrationBlunt>
				<linkedBodyPartsGroup>Handle</linkedBodyPartsGroup>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>blade</label>
				<capacities>
					<li>Cut</li>
				</capacities>
				<power>1.5</power>
				<cooldownTime>0.1</cooldownTime>
				<chanceFactor>0.4</chanceFactor>
				<armorPenetrationBlunt>0.5</armorPenetrationBlunt>
				<armorPenetrationSharp>1.5</armorPenetrationSharp>
				<linkedBodyPartsGroup>Blade</linkedBodyPartsGroup>
			</li>
		</tools>
        <modExtensions>
			<li Class="SurvivalToolsLite.SurvivalToolProperties">
                <baseWorkStatFactors>
                    <TreeFellingSpeed>2</TreeFellingSpeed>
                </baseWorkStatFactors>
                <defaultSurvivalToolAssignmentTags>
                    <li>PlantWorker</li>
                </defaultSurvivalToolAssignmentTags>
				<toolWearFactor>0.25</toolWearFactor>
            </li>
		</modExtensions>
		<recipeMaker>
			<recipeUsers>
				<li>ElectricSmithy</li>
